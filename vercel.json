{
  "version": 2,
  "builds": [
    { "src": "backend/index.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/polyfill.js", "dest": "/backend/index.js" },
    { "src": "/demo", "dest": "/demo.html" },
    { "src": "/", "dest": "/dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
